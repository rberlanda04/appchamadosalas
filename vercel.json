{
  "buildCommand": "npm run build",
  "outputDirectory": "frontend/build",
  "env": {
    "CI": "false",
    "GENERATE_SOURCEMAP": "false"
  },
  "functions": {
    "backend/server.js": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/backend/server.js"
    }
  ]
}